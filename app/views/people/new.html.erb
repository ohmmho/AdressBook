<%= form_for [@person], :html => { :multipart => true } do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :adress %>
  <%= f.text_field :adress %>
  <br>
  <%= f.label :phone %>
  <%= f.telephone_field :phone %>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>


  <%= f.submit 'Save' %>
  <%= link_to('Cancel', people_path) %>
  <% end %>