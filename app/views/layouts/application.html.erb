<!DOCTYPE html>
<html>
<head>
  <title>AdressBook</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<header>
<div class= "header">
  <div class="container">
    <div class="row">

      <div class="col-sm-3">
      
      </div>
        
      <div class="col-sm-6">
      <h1> COOL ADRESS BOOK! </h1>
      </div>

      <div class="col-sm-3">
        <%= form_tag search_path do %>
          <p>
            <%= text_field_tag :search %>
            <%= submit_tag "Search", :name => nil %>
          </p>
        <% end %>
      </div>

      </div>
    </div>
  </div>
  
</header>

<body>


<%= yield %>

</body>

<footer>
  derechos reservados 
</footer>
</html>
